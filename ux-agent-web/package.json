{
  "name": "ux-agent-web",
  "version": "0.0.0",
  "description": "",
  "license": "UNLICENSED",
  "author": "Fantatic Four",
  "type": "module",
  "engines": {
    "node": "22.18.0",
    "pnpm": "10.11.0",
    "npm": "please use pnpm instead"
  },
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typegen:api": "openapi-typescript './api/json-placeholder-openapi.yaml' -o './app/_types/jsonPlaceholderOpenapi.d.ts' -t",
    "typegen:css": "tcm app",
    "dev:next": "next dev --turbopack -p 4000",
    "build": "next build --turbopack",
    "dev": "pnpm typegen:css -w & pnpm dev:next & pnpm typecheck --watch",
    "start": "next start",
    "lint": "  prettier -c './**/*.@(js|cjs|ts|tsx|md)';    eslint './**/*.@(js|cjs|ts|tsx)';       stylelint './**/*.css';",
    "format": "prettier -c './**/*.@(js|cjs|ts|tsx|md)' -w; eslint './**/*.@(js|cjs|ts|tsx)' --fix; stylelint './**/*.css' --fix;",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build --stats-json",
    "test": "vitest"
  },
  "dependencies": {
    "@moneyforward/mfui-components": "2.0.1",
    "@moneyforward/mfui-design-tokens": "2.0.0",
    "@moneyforward/mfui-icons-react": "2.0.0",
    "jotai": "2.12.5",
    "next": "15.3.3",
    "openapi-fetch": "0.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "9.0.4",
    "@storybook/addon-links": "9.0.4",
    "@storybook/nextjs": "9.0.4",
    "@storybook/react": "9.0.4",
    "@types/node": "22.15.29",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "eslint": "9.28.0",
    "eslint-config-moneyforward": "5.1.1",
    "eslint-config-prettier": "10.1.5",
    "openapi-typescript": "7.8.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "storybook": "9.0.4",
    "stylelint": "16.20.0",
    "stylelint-config-moneyforward": "2.0.0",
    "typed-css-modules": "0.9.1",
    "typescript": "5.8.3",
    "vitest": "3.2.1"
  }
}
