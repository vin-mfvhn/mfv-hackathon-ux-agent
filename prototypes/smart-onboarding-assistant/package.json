{
  "name": "@moneyforward/smart-onboarding-assistant",
  "version": "1.0.0",
  "description": "Smart Onboarding Assistant - AI-powered tool for creating personalized onboarding checklists and tracking progress",
  "license": "UNLICENSED",
  "author": "@moneyforward/DPE",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typegen:api": "openapi-typescript './api/json-placeholder-openapi.yaml' -o './src/types/jsonPlaceholderOpenapi.d.ts' -t",
    "typegen:css": "tcm src",
    "develop": "vite",
    "start": "pnpm typegen:css -w & pnpm develop & pnpm typecheck --watch",
    "build": "pnpm typegen:css && pnpm typecheck && vite build",
    "preview": "vite preview",
    "lint": "  prettier -c './**/*.@(js|cjs|ts|tsx|md)';    eslint './**/*.@(js|cjs|ts|tsx)';       stylelint './**/*.css';",
    "format": "prettier -c './**/*.@(js|cjs|ts|tsx|md)' -w; eslint './**/*.@(js|cjs|ts|tsx)' --fix; stylelint './**/*.css' --fix;",
    "storybook": "storybook dev -p 6007",
    "build:storybook": "storybook build --stats-json",
    "test": "vitest"
  },
  "dependencies": {
    "@moneyforward/mfui-components": "2.0.1",
    "@moneyforward/mfui-design-tokens": "2.0.0",
    "@moneyforward/mfui-icons-react": "2.0.0",
    "clsx": "2.1.1",
    "constate": "3.3.3",
    "openapi-fetch": "0.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.6.2"
  },
  "devDependencies": {
    "@storybook/addon-docs": "9.0.4",
    "@storybook/addon-links": "9.0.4",
    "@storybook/react": "9.0.4",
    "@storybook/react-vite": "9.0.4",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "@vitejs/plugin-react": "4.5.1",
    "eslint": "9.28.0",
    "eslint-config-moneyforward": "5.1.1",
    "eslint-config-prettier": "10.1.5",
    "openapi-typescript": "7.8.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "storybook": "9.0.4",
    "storybook-addon-remix-react-router": "5.0.0",
    "stylelint": "16.20.0",
    "stylelint-config-moneyforward": "2.0.0",
    "typed-css-modules": "0.9.1",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.2.1"
  }
}
